        "sections": [
            {
                "id": "\u0627\u0644\u0642\u0633\u0645 \u0661",
                "title": "\u0646\u0638\u0631\u0629 \u0639\u0627\u0645\u0629",
                "statistic": {
                    "number": "\u06f8\u06f0 \u0645\u0644\u064a\u0648\u0646",
                    "text": "\u0623\u0639\u0636\u0627\u0621"
                },
                "fact": "\u062a\u0645\u062a\u062f \u0634\u0628\u0643\u0629 \u0627\u0644\u0627\u062a\u062d\u0627\u062f \u0627\u0644\u062f\u0648\u0644\u064a \u0644\u062c\u0645\u0639\u064a\u0627\u062a \u0627\u0644\u0635\u0644\u064a\u0628 \u0627\u0644\u0623\u062d\u0645\u0631 \u0648\u0627\u0644\u0647\u0644\u0627\u0644 \u0627\u0644\u0623\u062d\u0645\u0631 \u0625\u0644\u0649 \u0661\u0669\u0660 \u062f\u0648\u0644\u0629.",
                "quote": "\u064a\u0633\u062a\u0631\u0634\u062f \u0639\u0645\u0644\u0646\u0627 \u0643\u0644\u0647 \u0628\u0645\u0628\u0627\u062f\u0626\u0646\u0627 \u0627\u0644\u0623\u0633\u0627\u0633\u064a\u0629 \u0627\u0644\u062a\u064a \u0627\u0633\u062a\u062e\u0644\u0635\u0646\u0627\u0647\u0627 \u0645\u0646 \u062a\u062c\u0631\u0628\u062a\u0646\u0627 \u0627\u0644\u062c\u0645\u0627\u0639\u064a\u0629 \u0627\u0644\u0645\u062a\u0631\u0627\u0643\u0645\u0629 \u0639\u0644\u0649 \u0645\u062f\u0649 \u0623\u0643\u062b\u0631 \u0645\u0646 \u0642\u0631\u0646 \u0645\u0646 \u0627\u0644\u0632\u0645\u0627\u0646."
            },
            {
                "id": "\u0627\u0644\u0642\u0633\u0645 \u0662",
                "title": "\u0627\u0644\u0623\u0647\u062f\u0627\u0641 \u0627\u0644\u0627\u0633\u062a\u0631\u0627\u062a\u064a\u062c\u064a\u0629",
                "statistic": {
                    "number": "\u0661\u0661\u0660 \u0645\u0644\u064a\u0648\u0646",
                    "text": "\u0634\u062e\u0635 \u0645\u0634\u0645\u0648\u0644 \u0628\u0628\u0631\u0627\u0645\u062c \u0645\u0648\u0627\u062c\u0647\u0629 \u0627\u0644\u0643\u0648\u0627\u0631\u062b \u0648\u0627\u0644\u0627\u0646\u062a\u0639\u0627\u0634 \u0627\u0644\u0645\u0628\u0643\u0631"
                },
                "fact": "\u062a\u0645\u062a\u062f \u0634\u0628\u0643\u0629 \u0627\u0644\u0627\u062a\u062d\u0627\u062f \u0627\u0644\u062f\u0648\u0644\u064a \u0644\u062c\u0645\u0639\u064a\u0627\u062a \u0627\u0644\u0635\u0644\u064a\u0628 \u0627\u0644\u0623\u062d\u0645\u0631 \u0648\u0627\u0644\u0647\u0644\u0627\u0644 \u0627\u0644\u0623\u062d\u0645\u0631 \u0625\u0644\u0649 \u0661\u0669\u0660 \u062f\u0648\u0644\u0629.",
                "quote": "\u0648\u0639\u064f\u0631\u0641 \u0627\u0644\u0635\u0644\u064a\u0628 \u0627\u0644\u0623\u062d\u0645\u0631 \u0648\u0627\u0644\u0647\u0644\u0627\u0644 \u0627\u0644\u0623\u062d\u0645\u0631\u060c \u0645\u0646\u0630 \u0627\u0644\u0623\u064a\u0627\u0645 \u0627\u0644\u0623\u0648\u0644\u0649 \u0644\u062a\u0623\u0633\u064a\u0633\u0647\u060c \u0648\u0647\u0648 \u0628\u0645\u0647\u0645\u062a\u0647 \u0627\u0644\u0645\u062a\u0645\u062b\u0644\u0629 \u0641\u064a \u062a\u0642\u062f\u064a\u0645 \u0627\u0644\u0645\u0633\u0627\u0639\u062f\u0627\u062a \u0627\u0644\u0639\u0627\u062c\u0644\u0629 \u0648\u062a\u0648\u0641\u064a\u0631 \u0627\u0644\u0631\u0627\u062d\u0629 \u0641\u064a \u0648\u0642\u062a \u0627\u0644\u0634\u062f\u0629 \u0644\u0645\u0646 \u0647\u0645 \u0641\u064a \u0623\u0645\u0633 \u0627\u0644\u062d\u0627\u062c\u0629 \u0625\u0644\u064a\u0647\u0627. "
            },
            {
                "id": "\u0627\u0644\u0642\u0633\u0645 \u0663",
                "title": "\u0623\u0641\u0639\u0627\u0644 \u0627\u0644\u062a\u0645\u0643\u064a\u0646",
                "statistic": {
                    "number": "\u0661\u0669\u0660",
                    "text": "\u0627\u0644\u062c\u0645\u0639\u064a\u0627\u062a \u0627\u0644\u0648\u0637\u0646\u064a\u0629"
                },
                "fact": "\u0625\u0644\u0647\u0627\u0645\u060c \u0648\u062a\u0639\u0632\u064a\u0632\u060c \u0648\u062a\u0633\u0647\u064a\u0644\u060c \u0648\u062a\u0634\u062c\u064a\u0639 \u0645\u062e\u062a\u0644\u0641 \u0623\u0634\u0643\u0627\u0644 \u0627\u0644\u0646\u0634\u0627\u0637\u0627\u062a \u0627\u0644\u0625\u0646\u0633\u0627\u0646\u064a\u0629 \u0644\u0644\u062c\u0645\u0639\u064a\u0627\u062a \u0627\u0644\u0648\u0637\u0646\u064a\u0629 \u0641\u064a \u0643\u0644 \u0648\u0642\u062a\u060c \u0628\u0647\u062f\u0641 \u0645\u0646\u0639 \u0648 \u062a\u062e\u0641\u064a\u0641 \u0627\u0644\u0645\u0639\u0627\u0646\u0627\u0629 \u0627\u0644\u0625\u0646\u0633\u0627\u0646\u064a\u0629\u060c \u0648\u0628\u0627\u0644\u062a\u0627\u0644\u064a \u0627\u0644\u0645\u0633\u0627\u0647\u0645\u0629 \u0641\u064a \u0627\u0644\u062d\u0641\u0627\u0638 \u0639\u0644\u0649 \u0648\u0627\u0644\u0646\u0647\u0648\u0636 \u0628\u0643\u0631\u0627\u0645\u0629 \u0627\u0644\u0625\u0646\u0633\u0627\u0646 \u0648\u0627\u0644\u0633\u0644\u0627\u0645 \u0641\u064a \u0627\u0644\u0639\u0627\u0644\u0645.",
                "quote": "\u0648\u0645\u0646\u0630 \u0627\u0639\u062a\u0645\u0627\u062f \u0627\u0644\u0627\u0633\u062a\u0631\u0627\u062a\u064a\u062c\u064a\u0629 \u062d\u062a\u0649 \u0639\u0627\u0645 \u0662\u0660\u0662\u0660\u060c \u062c\u0631\u0649 \u062a\u0639\u0632\u064a\u0632 \u0645\u0639\u0638\u0645 \u0627\u0644\u062c\u0647\u0648\u062f \u0627\u0644\u0631\u0627\u0645\u064a\u0629 \u0625\u0644\u0649 \u062a\u062d\u0633\u064a\u0646 \u0639\u0645\u0644\u0646\u0627\u060c \u062d\u064a\u062b \u0631\u0643\u0632 \u0627\u0644\u0627\u062a\u062d\u0627\u062f \u0627\u0644\u062f\u0648\u0644\u064a \u0639\u0644\u0649 \u062a\u0639\u0632\u064a\u0632 \u0625\u062f\u0627\u0631\u062a\u0646\u0627\u060c \u0648\u0625\u062f\u0631\u0627\u062c \u0625\u0637\u0627\u0631 \u0644\u0644\u0645\u0633\u0627\u0621\u0644\u0629\u060c \u0648\u062a\u0639\u0632\u064a\u0632 \u062b\u0642\u0627\u0641\u0629 \u0627\u0644\u062a\u0639\u0644\u0645."
            },

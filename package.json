{
  "name": "everyone-counts",
  "version": "0.1.0",
  "description": "Everyone Counts 2015 Progress Report",
  "main": "index.js",
  "scripts": {
    "start": "concurrently --kill-others \"node index\" \"npm run development\"",
    "development": "webpack --progress --colors --watch",
    "production": "NODE_ENV=production webpack -p",
    "scss": "node_modules/.bin/node-sass client/styles/styles.scss public/styles/styles.css --watch",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "angular",
    "webpack",
    "es6"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/LapidusInteractive/a-vs-a_website"
  },
  "engines": {
    "node": "5.9.1",
    "npm": "2.13.3"
  },
  "authors": [
    "Arek Mytych",
    "Richard Zimerman"
  ],
  "private": true,
  "license": "Apache-2.0",
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "eslint-config-i-am-meticulous/react"
    ],
    "rules": {
      "react/prefer-stateless-function": 0,
      "max-len": [
        2,
        80,
        {
          "ignoreUrls": true,
          "ignoreStrings": true,
          "ignoreTemplateLiterals": true
        }
      ]
    }
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "brfs": "^1.4.3",
    "classnames": "^2.2.5",
    "d3": "^4.3.0",
    "d3-geo-projection": "^1.2.1",
    "datamaps": "^0.5.8",
    "express": "^4.14.0",
    "i18next": "^4.0.0",
    "i18next-xhr-backend": "^1.2.1",
    "lodash": "^4.17.2",
    "morgan": "^1.7.0",
    "object-assign": "^4.1.0",
    "promise-polyfill": "^6.0.2",
    "react": "^15.4.0",
    "react-button-group": "^1.0.2",
    "react-dom": "^15.4.0",
    "react-i18next": "^1.10.1",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "react-select": "^1.0.0-rc.2",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "serve-favicon": "^2.3.2",
    "superagent": "^2.3.0",
    "victory": "^0.13.7"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "concurrently": "^3.1.0",
    "eslint": "^3.10.2",
    "eslint-config-i-am-meticulous": "^5.0.2",
    "eslint-plugin-react": "^6.7.1",
    "gulp": "^3.9.1",
    "gulp-sass": "^2.3.2",
    "node-sass": "^3.13.0",
    "webpack": "^1.13.3"
  }
}
